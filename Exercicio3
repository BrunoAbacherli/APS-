package aps;

import java.util.Scanner;

public class Exercicio3 {

    public static void main(String[] args) {
        int num;
        Scanner entrada = new Scanner(System.in);
        for (num = 1000; num <= 1999; num++) {
            if (num % 11==5) {
                System.out.println("Os números são: " + num);
            }
        }
    }

}
